# FinTrack Environment Variables
# Copy this file to .env.local and fill in your values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# For migrations (optional - only needed if running migrations via script)
SUPABASE_DB_URL=postgres://postgres.your-project:your-password@aws-1-us-east-1.pooler.supabase.com:5432/postgres

# Groq AI Configuration
GROQ_API_KEY=your-groq-api-key
